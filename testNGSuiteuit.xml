<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Regression Suite" configfailurepolicy="continue" allow-return-values="true">
    <listeners>
<!--         <listener class-name="my.app.MyTestTransformer" /> -->
<!--         <listener class-name="my.app.MyMethodInterceptor" />		 -->
        <listener class-name="my.app.MyMultiListener" />		
    </listeners>

    <parameter name="fname" value="IME" />
    <parameter name="lname" value="PREZIME" />
    <parameter name="contextPrameter"  value="value from the SUITE context"/>
    
  	<test name = "TestNG Train 10 Dependency Injection">
  	    <parameter name="contextPrameter"  value="value from the TEST context"/>
		<classes>
	    	<class name="my.app.TestNGTrainingDI" />
		</classes>
    </test>
 
   	<test name = "TestNG Train 10 Dependency Injection 2">
  	    <parameter name="contextPrameter"  value="value from the TEST context 2"/>
		<classes>
	    	<class name="my.app.TestNGTrainingDI">
	    	<methods>
	    	<exclude name = "withInjection2" />
	    	</methods>
	    	</class>
		</classes>
    </test>
 
 </suite>